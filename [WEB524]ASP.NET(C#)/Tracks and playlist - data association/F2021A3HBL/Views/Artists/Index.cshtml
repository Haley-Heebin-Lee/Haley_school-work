@model IEnumerable<F2021A3HBL.Models.ArtistBaseViewModel>

@{
    ViewBag.Title = "Index";
}

<h2>List of Artists</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Name)
        </th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Name)
        </td>
    </tr>
}

</table>
